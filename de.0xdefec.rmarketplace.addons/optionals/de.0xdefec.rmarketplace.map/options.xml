<?xml version="1.0"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTD/options.dtd">
<data>
	<import>
		<categories>
			<category name="de.0xdefec.rmarketplace.gmaps.large">
				<parent>de.0xdefec.rmarketplace.gmaps</parent>
			</category>
