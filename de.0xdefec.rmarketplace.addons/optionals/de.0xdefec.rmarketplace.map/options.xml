		<options>
			<option name="MP_GMAP_LARGE_ENABLED">
				<categoryname>de.0xdefec.rmarketplace.gmaps.large</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>MP_GMAP_HIDE_OLD,MP_GMAP_LARGE_START_SET,MP_GMAP_LARGE_TYPE,MP_GMAP_LARGE_ZOOM</enableoptions>
			</option>
			<option name="MP_GMAP_LARGE_HIDE_OLD">
				<categoryname>de.0xdefec.rmarketplace.gmaps.large</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="MP_GMAP_LARGE_START_SET">
				<categoryname>de.0xdefec.rmarketplace.gmaps.large</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<!-- enableoptions>MP_GMAP_LARGE_LAT,MP_GMAP_LARGE_LNG</enableoptions -->
			</option>
			<option name="MP_GMAP_LARGE_LAT">
				<categoryname>de.0xdefec.rmarketplace.gmaps.large</categoryname>
				<optiontype>text</optiontype>
